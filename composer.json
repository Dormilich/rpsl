{
    "name": "dormilich/rpsl",
    "description": "Define base functionality for RPSL (whois) objects",
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Bertold von Dormilich",
            "email": "dormilich@netscape.net",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-SimpleXML": "*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Dormilich\\RPSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dormilich\\RPSL\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
